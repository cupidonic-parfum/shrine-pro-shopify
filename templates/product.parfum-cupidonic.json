        "custom_liquid_zpREaa": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<!-- üî¢ S√©lecteur de quantit√© - m√™me largeur que le bouton ATC (Impact) -->\n<div class=\"nk-qty-wrapper\">\n  <label class=\"nk-qty-label\" for=\"nk-qty-input\">Quantit√©</label>\n  <div class=\"nk-qty\">\n    <button type=\"button\" class=\"nk-qty-btn nk-qty-minus\" aria-label=\"R√©duire\">‚àí</button>\n    <input\n      id=\"nk-qty-input\"\n      type=\"number\"\n      name=\"quantity\"\n      value=\"1\"\n      min=\"1\"\n      class=\"nk-qty-input\"\n    >\n    <button type=\"button\" class=\"nk-qty-btn nk-qty-plus\" aria-label=\"Augmenter\">+</button>\n  </div>\n</div>\n\n<style>\n  .nk-qty-wrapper { margin: 1rem 0; width: 100%; }\n  .nk-qty-label { display:block; margin-bottom:6px; font-size:14px; color:#222; }\n  .nk-qty { display:grid; grid-template-columns:40px 1fr 40px; align-items:center; background:#F3FAFA; border-radius:8px; overflow:hidden; width:100%; }\n  .nk-qty-btn { height:44px; border:none; background:transparent; font-size:20px; font-weight:500; color:#000; cursor:pointer; display:flex; align-items:center; justify-content:center; }\n  .nk-qty-input { border:none; background:transparent; text-align:center; font-size:16px; height:44px; width:100%; appearance:textfield; }\n  .nk-qty-input::-webkit-outer-spin-button,.nk-qty-input::-webkit-inner-spin-button{ -webkit-appearance:none; margin:0; }\n</style>\n\n<script>\n(function(){\n  const root = document.currentScript.closest('.nk-qty-wrapper') || document;\n  const input = root.querySelector('#nk-qty-input');\n  const minus = root.querySelector('.nk-qty-minus');\n  const plus  = root.querySelector('.nk-qty-plus');\n\n  const MIN = () => Number(input.min) || 1;\n  const MAX = () => Number(input.max) || 9999;\n  const clamp = v => Math.max(MIN(), Math.min(v, MAX()));\n\n  function setVal(v){\n    input.value = clamp(Number(v)||MIN());\n    syncAllForms();\n    toggleMinus();\n  }\n  function toggleMinus(){ minus.disabled = (Number(input.value)||1) <= MIN(); }\n\n  minus.addEventListener('click', ()=> setVal((Number(input.value)||1)-1));\n  plus.addEventListener('click',  ()=> setVal((Number(input.value)||1)+1));\n  input.addEventListener('input', ()=> setVal(input.value));\n\n  // === SYNC ‚Üí pousse la valeur vers TOUS les formulaires add-to-cart visibles (Impact + sticky bar)\n  function getATCForms(){\n    return Array.from(document.querySelectorAll('form[action*=\"/cart/add\"]'))\n      .filter(f => f.offsetParent !== null);\n  }\n  function ensureHiddenQty(form){\n    // Certains th√®mes n'ont pas d'input quantity dans le form ‚Üí on le cr√©e cach√©\n    let q = form.querySelector('input[name=\"quantity\"]');\n    if(!q){\n      q = document.createElement('input');\n      q.type = 'hidden';\n      q.name = 'quantity';\n      form.appendChild(q);\n    }\n    return q;\n  }\n  function syncAllForms(){\n    const val = String(clamp(Number(input.value)||1));\n    getATCForms().forEach(form=>{\n      // 1) Mettez √† jour tout input[name=\"quantity\"] existant (visible ou non)\n      form.querySelectorAll('input[name=\"quantity\"]').forEach(el => el.value = val);\n      // 2) S‚Äôil n‚Äôy en a pas, cr√©ez le hidden qty (certaines impl√©mentations JS lisent directement dans le form)\n      ensureHiddenQty(form).value = val;\n    });\n  }\n\n  // === HOOK SUBMIT ‚Üí avant soumission (ou click ATC), on force la quantit√©\n  function hookForm(form){\n    if(form.__nkHooked) return;\n    form.__nkHooked = true;\n\n    // Sur submit classique\n    form.addEventListener('submit', (e)=>{\n      syncAllForms(); // s'assure que la valeur part bien\n    }, true);\n\n    // Sur clic du bouton d'ajout (certains th√®mes lisent la valeur √† ce moment)\n    const addBtn = form.querySelector('[type=\"submit\"][name=\"add\"], button[name=\"add\"]');\n    if(addBtn){\n      addBtn.addEventListener('click', ()=> syncAllForms(), true);\n    }\n  }\n\n  // Associer aussi l'input √† un form (attribut \"form\") pour compatibilit√© large\n  (function associateToNearestForm(){\n    const firstForm = getATCForms()[0];\n    if(firstForm){\n      if(!firstForm.id) firstForm.id = 'product-form-' + Math.random().toString(36).slice(2);\n      input.setAttribute('form', firstForm.id);\n    }\n  })();\n\n  // Initial sync + hook\n  syncAllForms();\n  getATCForms().forEach(hookForm);\n  toggleMinus();\n\n  // Variante change ‚Üí on peut remettre √† 1 si souhait√© et resync\n  document.addEventListener('variant:change', ()=>{\n    setVal(1);\n  });\n\n  // Si le th√®me remplace dynamiquement le form (AJAX sections), on re-hook/sync\n  const mo = new MutationObserver(()=>{\n    getATCForms().forEach(hookForm);\n    syncAllForms();\n  });\n  mo.observe(document.body, {childList:true, subtree:true});\n})();\n</script>"
          }
